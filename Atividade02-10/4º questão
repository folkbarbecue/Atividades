package aula002;
import java.util.Scanner;

public class Login {
	double cm, m;
    Scanner tec = new Scanner(System.in);
	public void Entrar() {
		//Variaveis

		System.out.println("Coloque um valor em metros:");
		m = tec.nextDouble();
		convert();


  }
	private void convert() {
      cm = m*100;
		System.out.println("O valor em centimetros Ã© de: "+cm);
	}
}
